<div class="container">
  <div class="row justify-content-center">
    <div class="col-sm-8">
      <div id="users">
        <% @users.each do |user| %>
          <h4><%= user.first_name %> <%= user.last_name %></h4>
          <p><%= user.nickname %></p>
        <% end %>
      </div>
    </div>
  </div>
</div>
