<div class="d-block show col-md-auto col-md-auto <%= myclass %>" style="margin-top: -170px;">
    <h1><%= label %></h1>
    <ul class="nav nav-tabs user-tasks-navbar btn-group" id="myTab" role="tablist">
      <li class="nav-item btn-success col-4" role="presentation">
        <a class="nav-link user-tasks-navbar-tab active" id="team1-tab" data-toggle="tab" href="#team1" role="tab" aria-controls="team1" aria-selected="true"><%= tab_one %></a>
      </li>
      <li class="nav-item btn-success col-4" role="presentation">
        <a class="nav-link user-tasks-navbar-tab" id="team2-tab" data-toggle="tab" href="#team2" role="tab" aria-controls="team2" aria-selected="false"><%= tab_two %></a>
      </li>
      <li class="nav-item btn-success col-4" role="presentation">
        <a class="nav-link user-tasks-navbar-tab" id="team3-tab" data-toggle="tab" href="#team3" role="tab" aria-controls="team3" aria-selected="false"><%= tab_three %></a>
      </li>
    </ul>

    <div class="tab-content" id="myTabContent">

      <div class="tab-pane fade show active" id="team1" role="tabpanel" aria-labelledby="team1-tab">
        <h2 style="margin-top: 20px"><%= message_tab_one %></h2>
        <div id="chart-<%= id_tab_one %>" style="height: 300px; margin-top: 40px" data-steps="<%= steps_tab_one.to_json %>"></div>
      </div>

      <div class="tab-pane fade" id="team2" role="tabpanel" aria-labelledby="team2-tab">
        <div>
          <h2 style="margin-top: 20px"><%= message_tab_two %></h2>
          <div id="chart-<%= id_tab_two %>" style="height: 300px; margin-top: 40px" data-steps="<%= steps_tab_two.to_json %>"></div>
        </div>
      </div>

      <div class="tab-pane fade" id="team3" role="tabpanel" aria-labelledby="team3-tab">
        <div>
          <p style="height: 400px; font-size: 200px">TO DO</p>
        </div>
      </div>

    </div>
  </div>
